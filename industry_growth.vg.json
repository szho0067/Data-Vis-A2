{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {
      "url": "https://raw.githubusercontent.com/szho0067/Data-Vis-A2/refs/heads/main/Data%20A2/industry_growth_average.csv"
    },
    "transform": [
    {
      "filter": "datum.Date == select_year"
    }
  ],
  "params": [
    {
      "name": "select_year",
      "value": 2014,
      "bind": {
        "input": "range",
        "min": 2014,
        "max": 2023,
        "step": 1,
        "name": "Select Year:"
      }
    }
  ],
    "mark": "line",
    "encoding": {
      "x": {
        "field": "Date",
        "type": "temporal",
        "title": "Year"
      },
      "y": {
        "field": "Average_Employed",
        "type": "quantitative",
        "title": "Average Employed"
      },
      "color": {
        "field": "Industry",
        "type": "nominal",
        "title": "Industry"
      }
    }
  }
  